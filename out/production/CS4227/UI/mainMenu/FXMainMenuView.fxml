<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" stylesheets="@../dark_theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.mainMenu.MainMenuController">
   <children>
      <GridPane layoutX="200.0" layoutY="160.0" prefWidth="200.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <JFXButton focusTraversable="false" onAction="#BookingButtonPushed" prefHeight="40.0" prefWidth="220.0" text="Make Booking" />
            <JFXButton focusTraversable="false" prefHeight="40.0" prefWidth="220.0" text="View Booking" GridPane.rowIndex="1" />
            <JFXButton focusTraversable="false" prefHeight="40.0" prefWidth="220.0" text="Edit Booking" GridPane.rowIndex="2" />
            <JFXButton fx:id="logoutButton" focusTraversable="false" onAction="#logoutButtonPushed" prefHeight="40.0" prefWidth="220.0" text="Logout" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
